struct PseudoClass {
    a, setA,
    b, setB
}

function createPseudoClassInstance (a) {
    var instance = new PseudoClass(a, "dummy", "dummy", "dummy");

    instance->setA = \(self, a) { self->a = a; self; } (instance);
    instance->setB = \(self, value) { self->b = value; self; } (instance);

    instance;
}

main {
    var instance = createPseudoClassInstance(100);
    return instance
        ->setA(100000)
        ->setB(123456)
        ->a + instance->b;
}